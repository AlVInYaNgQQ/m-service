{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/m-service/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/m-service/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/m-service",
  "projects": {
    "m-service": {
      "type": "application",
      "root": "apps/m-service",
      "entryFile": "main",
      "sourceRoot": "apps/m-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/m-service/tsconfig.app.json"
      }
    },
    "test": {
      "type": "application",
      "root": "apps/test",
      "entryFile": "main",
      "sourceRoot": "apps/test/src",
      "compilerOptions": {
        "tsConfigPath": "apps/test/tsconfig.app.json"
      }
    },
    "example": {
      "type": "library",
      "root": "libs/example",
      "entryFile": "index",
      "sourceRoot": "libs/example/src",
      "compilerOptions": {
        "tsConfigPath": "libs/example/tsconfig.lib.json"
      }
    }
  }
}